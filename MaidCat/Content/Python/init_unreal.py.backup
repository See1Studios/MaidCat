        # í˜„ì¬ ì„¤ì • ì½ê¸°
        with open(vscode_settings_path, 'r', encoding='utf-8') as f:
            # JSONì—ì„œ ì£¼ì„ ì œê±°ë¥¼ ìœ„í•œ ê°„ë‹¨í•œ ì²˜ë¦¬
            content = f.read()
            # // ì£¼ì„ ì œê±° (ê°„ë‹¨í•œ ë°©ë²•)
            lines = content.split('\n')
            clean_lines = []
            for line in lines:
                # // ì£¼ì„ì´ ìˆëŠ” ê²½ìš° ì œê±°
                if '//' in line:
                    comment_index = line.find('//')
                    # ë¬¸ìì—´ ë‚´ë¶€ì˜ //ì¸ì§€ í™•ì¸ (ê°„ë‹¨í•œ ê²€ì‚¬)
                    quote_count = line[:comment_index].count('"')
                    if quote_count % 2 == 0:  # ë¬¸ìì—´ ì™¸ë¶€ì˜ ì£¼ì„
                        line = line[:comment_index].rstrip()
                if line.strip():  # ë¹ˆ ì¤„ì´ ì•„ë‹Œ ê²½ìš°ì—ë§Œ ì¶”ê°€
                    clean_lines.append(line)
            
            clean_content = '\n'.join(clean_lines)
            settings = json.loads(clean_content)
        
        # Python ê²½ë¡œë“¤ ì„¤ì •
